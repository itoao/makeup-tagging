{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    // ここにプロジェクト固有のルールを追加できます
    "@typescript-eslint/no-unused-vars": "warn", // 未使用の変数は警告にする
    "@typescript-eslint/no-explicit-any": "warn" // any 型の使用は警告にする
  }
}
